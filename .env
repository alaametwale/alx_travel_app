# alx_travel_app/.env
# --- Django Settings ---
SECRET_KEY='django-insecure-YOUR_SUPER_SECRET_KEY'
DEBUG=True
ALLOWED_HOSTS=127.0.0.1,localhost

# --- Database Settings (MySQL) ---
# تأكد من أن قاعدة البيانات 'alxtraveldb' موجودة وأن لديك بيانات اعتماد صالحة
DATABASE_URL=mysql://[YOUR_MYSQL_USER]:[YOUR_MYSQL_PASSWORD]@[YOUR_MYSQL_HOST]:3306/alxtraveldb

# --- Other Settings (CORS, etc.) ---
CORS_ALLOWED_ORIGINS=http://localhost:3000